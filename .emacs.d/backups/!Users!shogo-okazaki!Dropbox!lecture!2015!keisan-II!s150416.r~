par(mfrow=c(1,2))
matplot(VADeaths, type="b")
RL <- nrow(VADeaths)
CL <- ncol(VADeaths)
matplot(VADeaths,type="b",pch=1:CL,xlab="",ylab="",axes=F,col=1:CL,lty=1:CL)
axis(1,1:RL,rownames(VADeaths))
axis(2)
legend(1,max(VADeaths), colnames(VADeaths), col=1:CL, lty=1:CL)

par(mfrow=c(1,1)) 
matplot(VADeaths,type="b",pch=1:CL,xlab="",ylab="",axes=F)
axis(1,1:RL,rownames(VADeaths)); axis(2)
legend(2,60, rownames(VADeaths), col=1:5, lty=1)      
legend(3,max(VADeaths), rownames(VADeaths), col=1:5, lty=1)  
legend("topleft", rownames(VADeaths), col=1:5, lty=1)  
