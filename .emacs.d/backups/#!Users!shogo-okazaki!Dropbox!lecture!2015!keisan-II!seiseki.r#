exam <- read.table("150423c.dat", header=T)

n <- nrow(exam)
In <- matrix(1,n,1)
Qn <- diag(1,n,n) - In %*% t(In) / n
exam <- as.matrix(exam)
S <- t(exam) %*% Qn %*% exam / n

sqrt(eigen(S)$values)
eigen(S)$values / sum(diag(S))
cumsum(eigen(S)$values / sum(diag(S)))

exam.pc$loadings
eigen(S)$vectors

