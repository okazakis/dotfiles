%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%%%%%%%%         New Command Definition        %%%%%%%%
\newcommand{\dd}{\mathrm{d}}
\newcommand{\ee}{\mathrm{e}}
\newcommand{\bnabla}{\bm{\nabla}}
\newcommand{\DD}[3][]{\frac{\dd^{#1} #2}{\dd #3^{#1}}}
\newcommand{\DDD}[3][]{\frac{\mathrm{D}^{#1} #2}{\mathrm{D} #3^{#1}}}
\newcommand{\DP}[3][]{\frac{\partial^{#1} #2}{\partial #3^{#1}}}
%------------------------------------------------------
% \aboxed{A}{=B} で数式を四角で囲むコマンド
%------------------------------------------------------
\newcommand{\aboxed}[2]{%
  \rlap{\boxed{#1#2}}%
  \phantom{\hskip\fboxrule\hskip\fboxsep #1}&\phantom{#2}%
}
% ----- 定理環境の設定 -----
\newtheorem{dfn}{定義}[section]
\newtheorem{thm}[dfn]{定理}
\newtheorem{lem}[dfn]{補題}


