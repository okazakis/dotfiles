import graph;

size(0,200);

real x(real t) {return cos(t)**(3/2);}
real y(real t) {return cos(t)**(1/2)*sin(t);}

real x2(real t) {return -cos(t)**(3/2);}
real y2(real t) {return -cos(t)**(1/2)*sin(t);}

draw(graph(x,y,-0.5pi,0.5pi));
//limits((-1,1),(-1,1),Crop);

draw(graph(x2,y2,-0.5pi,0.5pi));
//limits((-1,1),(-1,1),Crop);

xaxis("$x$",-0.5pi,0.5pi,LeftTicks(NoZero),Arrow);
yaxis("$y$",-0.5pi,0.5pi,RightTicks,Arrow);
